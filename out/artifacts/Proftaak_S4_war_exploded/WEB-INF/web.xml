<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         id="WebApp_ID" version="3.0">
    <servlet>
        <servlet-name>HomeController</servlet-name>
        <servlet-class>Pts4.Controllers.HomeController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HomeController</servlet-name>
        <url-pattern>/HomeController</url-pattern> 
    </servlet-mapping>

    <servlet>
        <servlet-name>TimestampController</servlet-name>
        <servlet-class>Pts4.Controllers.TimestampController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TimestampController</servlet-name>
        <url-pattern>/TimestampController</url-pattern> 
    </servlet-mapping>
</web-app>